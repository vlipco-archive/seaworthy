#!/bin/bash
cd $(dirname $0)
git init || :
git config --global user.email "you@example.com"
git config --global user.name "Your Name"
date > date
git add -A .
git commit -am "Fake commit"
git remote add tester ssh://git@localhost:5100/ruby
export GIT_SSH="$(pwd)/../git_ssh"
echo $GIT_SSH
git push tester master